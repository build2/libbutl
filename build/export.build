if ($name($import.target) == 'butl-odb')
{
  $out_root/
  {
    include libbutl-odb/
  }

  export $out_root/libbutl-odb/$import.target
}
else
{
  $out_root/
  {
    include libbutl/
  }

  export $out_root/libbutl/$import.target
}
